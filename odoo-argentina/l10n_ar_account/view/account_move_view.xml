<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_move_filter" model="ir.ui.view">
        <field name="name">account.move.filter</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_account_move_filter"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="afip_responsability_type_id"/>
            </field>
            <group>
                <filter name="afip_responsability_type_id" string="AFIP Responsability Type" domain="[]" context="{'group_by':'afip_responsability_type_id'}"/>
            </group>
        </field>
    </record>
</odoo>
